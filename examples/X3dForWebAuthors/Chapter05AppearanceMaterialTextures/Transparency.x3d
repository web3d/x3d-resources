  <Scene>
    <WorldInfo title='Transparency.x3d'/>
    <Background skyColor='1 1 1'/>
    <Viewpoint description='Book Viewpoint' orientation='-0.788 -0.614 -0.055 -0.12' position='-0.56 -0.64 3.37'/>
    <Transform>
      <Shape>
        <Appearance>
          <Material diffuseColor='1 0 0' specularColor='0 1 0' transparency='.4'/>
        </Appearance>
        <Sphere radius='.7'/>
      </Shape>
    </Transform>
    <Transform rotation='.707 .707 0 .707' translation='0 0 -6'>
      <Shape>
        <Appearance>
          <Material diffuseColor='0 0 1'/>
        </Appearance>
        <Box/>
      </Shape>
    </Transform>
    <Transform translation='0 1 -8'>
      <Shape>
        <Appearance>
          <Material diffuseColor='0 .6 0'/>
        </Appearance>
        <Cylinder height='8' radius='.4'/>
      </Shape>
    </Transform>
  </Scene>
