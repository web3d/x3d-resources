          <Coordinate point='-6 -2 -2 -6 -2 2 6 -2 2 6 -2 -2 -6 2 -2 -6 2 2 6 2 2 6 2 -2'/>
        </IndexedLineSet>
        <Appearance>
          <!-- lines are only lit by emissiveColor -->
          <Material emissiveColor='0 0.8 0.8'/>
        </Appearance>
      </Shape>
    </Group>
  </Scene>
