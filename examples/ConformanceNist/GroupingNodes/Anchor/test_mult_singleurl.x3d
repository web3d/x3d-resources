  <Scene>
    <WorldInfo title='test_mult_singleurl.x3d'/>
    <NavigationInfo type='"EXAMINE" "WALK" "FLY" "ANY"'/>
    <Viewpoint description='Front View' position='0 0 18'/>
    <Transform translation='0 2 0'>
      <Shape>
        <Appearance>
          <Material diffuseColor='1 0 0' emissiveColor='1 0 0'/>
        </Appearance>
        <Text string='"CLICK ANY SPHERE TO START"'>
          <FontStyle justify='"MIDDLE" "MIDDLE"'/>
        </Text>
      </Shape>
    </Transform>
    <Anchor description='Multiple chlidren referencing same URL' url='"singleurl.x3d" "https://www.web3d.org/x3d/content/examples/ConformanceNist/GroupingNodes/Anchor/singleurl.x3d" "singleurl.wrl" "https://www.web3d.org/x3d/content/examples/ConformanceNist/GroupingNodes/Anchor/singleurl.wrl"'>
      <Transform translation='-3 0 0'>
        <Shape>
          <Appearance>
            <Material diffuseColor='1 0 0' emissiveColor='1 0 0'/>
          </Appearance>
          <Sphere/>
        </Shape>
      </Transform>
      <Transform>
        <Shape>
          <Appearance>
            <Material diffuseColor='1 0 0' emissiveColor='1 0 0'/>
          </Appearance>
          <Sphere/>
        </Shape>
      </Transform>
      <Transform translation='3 0 0'>
        <Shape>
          <Appearance>
            <Material diffuseColor='1 0 0' emissiveColor='1 0 0'/>
          </Appearance>
          <Sphere/>
        </Shape>
      </Transform>
    </Anchor>
  </Scene>
</X3D>